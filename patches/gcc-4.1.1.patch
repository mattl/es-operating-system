+     *-*-freebsd6 | *-*-freebsd[6].*)
+       tm_defines="${tm_defines} FBSD_MAJOR=6" ;;
+     *-*-freebsd7 | *-*-freebsd[7].*)
+       tm_defines="${tm_defines} FBSD_MAJOR=7" ;;
+     *-*-freebsd8 | *-*-freebsd[8].*)
+       tm_defines="${tm_defines} FBSD_MAJOR=8" ;;
+     *-*-freebsd9 | *-*-freebsd[9].*)
