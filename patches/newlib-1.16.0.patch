+ pthread_key_create(pthread_key_t* key, void (*dtor)(void*))
+ {
+     pthread_key_t i;
+ 
+     pthread_once(&once, initMonitor);
+     if (!dtor)
+     {
+         dtor = defaultDtor;
+     }
+     pthread_mutex_lock(&monitor);
+     for (i = hint; i < PTHREAD_KEYS_MAX; ++i)
+     {
+         Key* key = &keys[i];
+         if (!key->dtor)
+         {
+             ++hint;
+             key->dtor = dtor;
+             break;
+         }
+     }
+     pthread_mutex_unlock(&monitor);
+     if (PTHREAD_KEYS_MAX <= i)
+     {
+         *key = -1;
+         return EAGAIN;
+     }
+     *key = i;
+     return 0;
+ }
+ 
+ int __attribute__((weak))
+ pthread_key_delete(pthread_key_t key)
+ {
+     if (PTHREAD_KEYS_MAX <= key)
+     {
+         return EINVAL;
+     }
+     pthread_mutex_lock(&monitor);
+     if (keys[key].dtor)
+     {
+         ++keys[key].seq;
+         keys[key].dtor = 0;
+         hint = key;
+     }
+     pthread_mutex_unlock(&monitor);
+     return 0;
+ }
+ 
+ void* __attribute__((weak))
+ pthread_getspecific(pthread_key_t key)
+ {
+     if (PTHREAD_KEYS_MAX <= key)
+     {
+         return 0;
+     }
+     return specific[key].data;
+ }
+ 
+ int __attribute__((weak))
+ pthread_setspecific(pthread_key_t key, const void* ptr)
+ {
+     int seq;
+ 
+     if (PTHREAD_KEYS_MAX <= key || !keys[key].dtor)
+     {
+         return EINVAL;
+     }
+     do
+     {
+         seq = keys[key].seq;
+         specific[key].data = (void*) ptr;
+         specific[key].seq = seq;
+     } while (seq != keys[key].seq);
+     return 0;
+ }
+ 
+ void esDeallocateSpecific(void)
+ {
+     pthread_key_t i;
+ 
+     for (i = 0; i < PTHREAD_KEYS_MAX; ++i)
+     {
+         KeyData* data = &specific[i];
+         if (data->data)
+         {
+             void* ptr = data->data;
+             Key* key = &keys[i];
+             void (*dtor)(void*) = key->dtor;
+ 
+             data->data = 0;
+             if (dtor && key->seq == data->seq)
+             {
+                 dtor(ptr);
+             }
+         }
+     }
+ }
+ 
+ static void* mutex_get_monitor(pthread_mutex_t* mutex)
+ {
+     if (mutex->monitor == 0)
+     {
+         pthread_once(&once, initMonitor);
+         pthread_mutex_lock(&monitor);
+         if (mutex->monitor == 0)
+         {
+             pthread_mutex_init(mutex, 0);
+         }
+         pthread_mutex_unlock(&monitor);
+     }
+     return mutex->monitor;
+ }
+ 
+ static int mutex_inc(pthread_mutex_t* mutex)
+ {
+     register int eax = 1;
+ 
+     __asm__ __volatile__(
+         "lock\n"
+         "xaddl  %0, %1\n"
+         "incl   %0\n"
+         : "=a" (eax), "=m" (mutex->busy)
+         : "0" (eax), "m" (mutex->busy));
+ 
+     return eax;
+ }
+ 
+ static int mutex_dec(pthread_mutex_t* mutex)
+ {
+     register int eax = -1;
+ 
+     __asm__ __volatile__(
+         "lock\n"
+         "xaddl  %0, %1\n"
+         "decl   %0\n"
+         : "=a" (eax), "=m" (mutex->busy)
+         : "0" (eax), "m" (mutex->busy));
+ 
+     return eax;
+ }
+ 
+ static int mutex_tas(pthread_mutex_t* mutex)
+ {
+     register int value = 1;
+     register int comparand = 0;
+ 
+     __asm__ __volatile__ (
+         "lock\n"
+         "cmpxchgl   %2, %1\n"
+         : "=a" (value), "=m" (mutex->lock)
+         : "r" (value), "0" (comparand), "m" (mutex->lock));
+ 
+     return value;
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutex_trylock(pthread_mutex_t* mutex)
+ {
+     int lock;
+ 
+     if (mutex->owner == pthread_self())
+     {
+         ++mutex->count;
+         return 0;
+     }
+ 
+     lock = mutex_tas(mutex);
+     if (lock == 0)
+     {
+         mutex->owner = pthread_self();
+         mutex->count = 1;
+         return 0;
+     }
+     return EBUSY;
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutex_lock(pthread_mutex_t* mutex)
+ {
+     int lock;
+     void* monitor;
+ 
+     for (;;)
+     {
+         mutex_inc(mutex);
+ 
+         if (pthread_mutex_trylock(mutex) == 0)
+         {
+             mutex_dec(mutex);
+             return 0;
+         }
+ 
+         monitor = mutex_get_monitor(mutex);
+         if (monitor)
+         {
+             _es_syscall(monitor, 0, LOCK, 0);
+             ++mutex->wait;
+             while (0 < mutex->wait)
+             {
+                 _es_syscall(monitor, 0, WAIT, 0);
+             }
+             _es_syscall(monitor, 0, UNLOCK, 0);
+         }
+ 
+         mutex_dec(mutex);
+     }
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutex_unlock(pthread_mutex_t* mutex)
+ {
+     int lock;
+     void* monitor;
+ 
+     if (0 < --mutex->count)
+     {
+         return 0;
+     }
+     mutex->owner = 0;
+     mutex->lock = 0;
+ 
+     if (mutex->busy)
+     {
+         monitor = mutex_get_monitor(mutex);
+         if (monitor)
+         {
+             _es_syscall(monitor, 0, LOCK, 0);
+             if (0 < mutex->wait)
+             {
+                 _es_syscall(monitor, 0, NOTIFY_ALL, 0);
+                 mutex->wait = 0;
+             }
+             else
+             {
+                 mutex->wait = -1;
+             }
+             _es_syscall(monitor, 0, UNLOCK, 0);
+         }
+     }
+ 
+     return 0;
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutexattr_init(pthread_mutexattr_t* attr)
+ {
+     return 0;
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutexattr_settype(pthread_mutexattr_t* attr, int type)
+ {
+     return 0;
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutexattr_destroy(pthread_mutexattr_t* attr)
+ {
+     return 0;
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutex_init(pthread_mutex_t* mutex, const pthread_mutexattr_t* attr)
+ {
+     mutex->monitor = (void*) _es_syscall(0, 0, CREATE_MONITOR, 0);
+     mutex->owner = 0;
+     mutex->busy = 0;
+     mutex->lock = 0;
+     mutex->wait = 0;
+     mutex->count = 0;
+     return 0;
+ }
+ 
+ int __attribute__((weak))
+ pthread_mutex_destroy(pthread_mutex_t* mutex)
+ {
+     void* monitor = mutex_get_monitor(mutex);
+     if (monitor)
+     {
+         _es_syscall(monitor, 0, RELEASE, 0);
+         mutex->monitor = 0;
+     }
+     return 0;
+ }
+ 
+ pthread_t __attribute__((weak))
+ pthread_self(void)
+ {
+     register int eax;
+ 
+     __asm__ __volatile__(
+         "mov    %%gs:0x0, %0\n" // Use TLS base as self for now
+         : "=a" (eax));
+ 
+     return eax;
+ }
