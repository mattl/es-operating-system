+    case 0: // stdin
+        if (!_es_streamStdin)
+        {
+            _es_streamStdin = (void*) _es_syscall(0, 0, GET_IN, 0);
+        }
+        object = _es_streamStdin;
+        break;
+    case 1: // stdout
+        if (!_es_streamStdout)
+        {
+            _es_streamStdout = (void*) _es_syscall(0, 0, GET_OUT, 0);
+        }
+        object = _es_streamStdout;
+        break;
+    case 2: // stderr
+        if (!_es_streamStderr)
+        {
+            _es_streamStderr = (void*) _es_syscall(0, 0, GET_ERROR, 0);
+        }
+        object = _es_streamStderr;
+        break;
+    default:
