+ 
+ int __attribute__((weak))
+ getdents(unsigned int fd, struct dirent* dirp, unsigned int count)
+ {
+     void* iter = _es_getInterfacePointer(fd);   // IIterator
+     void* unknown;
+     void* binding;
+     int len;
+ 
+     if (!dirp)
+     {
+         errno = EFAULT;
+         return -1;
+     }
+ 
+     if (count < sizeof(sizeof(struct dirent)))
+     {
+         errno = EINVAL;
+         return -1;
+     }
+ 
+     unknown = (void*) _es_syscall(iter, 0, NEXT, 0);
+     if (!unknown)
+     {
+         return 0;
+     }
+ 
+     binding = _es_queryInterface(unknown, &_es_bindingIid);
+     _es_release(unknown);
+     if (!binding)
+     {
+         errno = ENOTDIR;
+         return -1;
+     }
+     len = _es_getBindingName(binding, dirp->d_name, MAXNAMLEN);
+     _es_release(binding);
+ 
+     if (len < 0)
+     {
+         errno = ENOTDIR;
+         return -1;
+     }
+     dirp->d_off = 0;
+     dirp->d_fileno = len;   // Note: just as a non-zero value for readdir()
+     dirp->d_reclen = sizeof(struct dirent);
+     dirp->d_name[MAXNAMLEN] = '\0';
+     dirp->d_namlen = len;
+ 
+     return sizeof(struct dirent);
+ }
