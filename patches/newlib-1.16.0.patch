+
+#include <unistd.h>
+#include <sys/mman.h>
+
+#include <errno.h>
+#include "es-syscall.h"
+
+void* __attribute__((weak))
+mmap(void *start, size_t length, int prot, int flags, int fd, off_t offset)
+{
+    void* unknown = _es_getInterfacePointer(fd);
+    void* file;
+    void* pageable;
+    void* addr;
+
+    file = _es_queryInterface(unknown, _es_fileIid);
+    if (!file)
+    {
+        return (void*) -1;
+    }
+    pageable = (void*) _es_syscall(file, 0, GET_PAGEABLE, 0);
+    _es_release(file);
+    if (!pageable)
+    {
+        return (void*) -1;
+    }
+    addr = _es_map(start, length, prot, flags, pageable, offset);
+    _es_release(pageable);
+
+    return addr;
+}
+
+int __attribute__((weak))
+munmap(void *start, size_t length)
+{
+    _es_unmap(start, length);
+    return 0;
+}
+
+size_t __attribute__((weak))
+getpagesize(void)
+{
+    return 4096;
+}
