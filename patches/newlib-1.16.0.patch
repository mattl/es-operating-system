+     _es_syscall(current, 0, THREAD_SLEEP, (va_list) &time);
+     _es_release(current);
+ 
+     if (rem)
+     {
+         rem->tv_sec = 0;
+         rem->tv_nsec = 0;
+     }
+ 
+     return 0;
+ }
