+ void* _es_addRef(void* interface)
+ {
+     return (void*) _es_syscall(interface, 0, ADD_REF, 0);
+ }
+ 
+ void* _es_release(void* interface)
+ {
+     return (void*) _es_syscall(interface, 0, RELEASE, 0);
