+ * distributed under the License is distributed on an "AS IS" BASIS,
+ * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
+ * See the License for the specific language governing permissions and
+ * limitations under the License.
+ */
+
+#include "es-syscall.h"
+
+#include <assert.h>
+#include <errno.h>
+#include <pthread.h>
+#include <stdarg.h>
+#include <time.h>
+#include <unistd.h>
+
+#ifndef PTHREAD_KEYS_MAX
+#define PTHREAD_KEYS_MAX    64
+#endif
+
+typedef struct Key
+{
+    volatile int seq;
+    void (*dtor)(void*);
+} Key;
+
+typedef struct KeyData
+{
+    int seq;
+    void* data;
+} KeyData;
+
+static pthread_once_t   once = PTHREAD_ONCE_INIT;
+static pthread_mutex_t  monitor;
+static Key              keys[PTHREAD_KEYS_MAX];
+static unsigned int     hint;
+static __thread KeyData specific[PTHREAD_KEYS_MAX];
+
+int __attribute__((weak))
+pthread_once(pthread_once_t* once, void (*func) (void))
+{
+    if (!once->init_executed)
+    {
+        int initialized = 0;
+
+        __asm__ __volatile__ (
+            "xchgl  %0, %1\n"
+            : "=a"(initialized), "=m"(once->is_initialized)
+            : "0"(initialized), "m"(once->is_initialized));
+
+        if (initialized)
+        {
+            if (func)
+            {
+                func();
+            }
+            once->init_executed = 1;
+        }
+        else
+        {
+            while (!once->init_executed)
+            {
+                static struct timespec req = { 0, 100000000LL };
+
+                nanosleep(&req, 0);
+            }
+        }
+    }
+    return 0;
+}
+
+static void initMonitor(void)
+{
+    pthread_mutex_init(&monitor, 0);
+}
+
+static void defaultDtor(void* data)
+{
+}
+
+int __attribute__((weak))
+pthread_key_create(pthread_key_t* key, void (*dtor)(void*))
+{
+    pthread_key_t i;
+
+    pthread_once(&once, initMonitor);
+    if (!dtor)
+    {
+        dtor = defaultDtor;
+    }
+    pthread_mutex_lock(&monitor);
+    for (i = hint; i < PTHREAD_KEYS_MAX; ++i)
+    {
+        Key* key = &keys[i];
+        if (!key->dtor)
+        {
+            ++hint;
+            key->dtor = dtor;
+            break;
+        }
+    }
+    pthread_mutex_unlock(&monitor);
+    if (PTHREAD_KEYS_MAX <= i)
+    {
+        *key = -1;
+        return EAGAIN;
+    }
+    *key = i;
+    return 0;
+}
+
+int __attribute__((weak))
+pthread_key_delete(pthread_key_t key)
+{
+    if (PTHREAD_KEYS_MAX <= key)
+    {
+        return EINVAL;
+    }
+    pthread_mutex_lock(&monitor);
+    if (keys[key].dtor)
+    {
+        ++keys[key].seq;
+        keys[key].dtor = 0;
+        hint = key;
+    }
+    pthread_mutex_unlock(&monitor);
+    return 0;
+}
+
+void* __attribute__((weak))
+pthread_getspecific(pthread_key_t key)
+{
+    if (PTHREAD_KEYS_MAX <= key)
+    {
+        return 0;
+    }
+    return specific[key].data;
+}
+
+int __attribute__((weak))
+pthread_setspecific(pthread_key_t key, const void* ptr)
+{
+    int seq;
+
+    if (PTHREAD_KEYS_MAX <= key || !keys[key].dtor)
+    {
+        return EINVAL;
+    }
+    do
+    {
+        seq = keys[key].seq;
+        specific[key].data = (void*) ptr;
+        specific[key].seq = seq;
+    } while (seq != keys[key].seq);
+    return 0;
+}
+
+void esDeallocateSpecific(void)
+{
+    pthread_key_t i;
+
+    for (i = 0; i < PTHREAD_KEYS_MAX; ++i)
+    {
+        KeyData* data = &specific[i];
+        if (data->data)
+        {
+            void* ptr = data->data;
+            Key* key = &keys[i];
+            void (*dtor)(void*) = key->dtor;
+
+            data->data = 0;
+            if (dtor && key->seq == data->seq)
+            {
+                dtor(ptr);
+            }
+        }
+    }
+}
+
+static void* mutex_get_monitor(pthread_mutex_t* mutex)
+{
+    if (mutex->monitor == 0)
+    {
+        pthread_once(&once, initMonitor);
+        pthread_mutex_lock(&monitor);
+        if (mutex->monitor == 0)
+        {
+            pthread_mutex_init(mutex, 0);
+        }
+        pthread_mutex_unlock(&monitor);
+    }
+    return mutex->monitor;
+}
+
+static int mutex_inc(pthread_mutex_t* mutex)
+{
+    register int eax = 1;
+
+    __asm__ __volatile__(
+        "lock\n"
+        "xaddl  %0, %1\n"
+        "incl   %0\n"
+        : "=a" (eax), "=m" (mutex->busy)
+        : "0" (eax), "m" (mutex->busy));
+
+    return eax;
+}
+
+static int mutex_dec(pthread_mutex_t* mutex)
+{
+    register int eax = -1;
+
+    __asm__ __volatile__(
+        "lock\n"
+        "xaddl  %0, %1\n"
+        "decl   %0\n"
+        : "=a" (eax), "=m" (mutex->busy)
+        : "0" (eax), "m" (mutex->busy));
+
+    return eax;
+}
+
+static int mutex_tas(pthread_mutex_t* mutex)
+{
+    register int value = 1;
+    register int comparand = 0;
+
+    __asm__ __volatile__ (
+        "lock\n"
+        "cmpxchgl   %2, %1\n"
+        : "=a" (value), "=m" (mutex->lock)
+        : "r" (value), "0" (comparand), "m" (mutex->lock));
+
+    return value;
+}
+
+int __attribute__((weak))
+pthread_mutex_trylock(pthread_mutex_t* mutex)
+{
+    int lock;
+
+    if (mutex->owner == pthread_self())
+    {
+        ++mutex->count;
+        return 0;
+    }
+
+    lock = mutex_tas(mutex);
+    if (lock == 0)
+    {
+        mutex->owner = pthread_self();
+        mutex->count = 1;
+        return 0;
+    }
+    return EBUSY;
+}
+
+int __attribute__((weak))
+pthread_mutex_lock(pthread_mutex_t* mutex)
+{
+    int lock;
+    void* monitor;
+
+    for (;;)
+    {
+        mutex_inc(mutex);
+
+        if (pthread_mutex_trylock(mutex) == 0)
+        {
+            mutex_dec(mutex);
+            return 0;
+        }
+
+        monitor = mutex_get_monitor(mutex);
+        if (monitor)
+        {
+            _es_syscall(monitor, 0, LOCK, 0);
+            ++mutex->wait;
+            while (0 < mutex->wait)
+            {
+                _es_syscall(monitor, 0, WAIT, 0);
+            }
+            _es_syscall(monitor, 0, UNLOCK, 0);
+        }
+
+        mutex_dec(mutex);
+    }
+}
+
+int __attribute__((weak))
+pthread_mutex_unlock(pthread_mutex_t* mutex)
+{
+    int lock;
+    void* monitor;
+
+    if (0 < --mutex->count)
+    {
+        return 0;
+    }
+    mutex->owner = 0;
+    mutex->lock = 0;
+
+    if (mutex->busy)
+    {
+        monitor = mutex_get_monitor(mutex);
+        if (monitor)
+        {
+            _es_syscall(monitor, 0, LOCK, 0);
+            if (0 < mutex->wait)
+            {
+                _es_syscall(monitor, 0, NOTIFY_ALL, 0);
+                mutex->wait = 0;
+            }
+            else
+            {
+                mutex->wait = -1;
+            }
+            _es_syscall(monitor, 0, UNLOCK, 0);
+        }
+    }
+
+    return 0;
+}
