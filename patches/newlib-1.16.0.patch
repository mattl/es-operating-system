+        mutex->monitor = 0;
+    }
+    return 0;
+}
+
+pthread_t __attribute__((weak))
+pthread_self(void)
+{
+    register int eax;
+
+    __asm__ __volatile__(
+        "mov    %%gs:0x0, %0\n" // Use TLS base as self for now
+        : "=a" (eax));
+
+    return eax;
+}
+
+int pthread_create(pthread_t* pthread, const pthread_attr_t* attr, void* (*start_routine)(void*), void* arg)
+{
+    int rc = _es_syscall(0, 0, CREATE_THREAD, (va_list) &start_routine);
+    if (rc != -1)
+    {
+        pthread_t thread = (pthread_t) rc;
+        if (thread)
+        {
+            if (pthread)
+            {
+                *pthread = thread;
+            }
+            _es_syscall((void*) thread, 0, START, 0);
+            return 0;
+        }
+        errno = EAGAIN;
+    }
+    return -1;
+}
+
+int pthread_join(pthread_t pthread, void** value_ptr)
+{
+    int rc = _es_syscall((void*) pthread, 0, JOIN, 0);
+    if (rc == -1)
+    {
+        return -1;
+    }
+    void* value = (void*) rc;
+    if (value_ptr)
+    {
+        *value_ptr = value;
+    }
+    _es_release((void*) pthread);
+    return 0;
+}
+
+int pthread_detach(pthread_t pthread)
+{
+    _es_release((void*) pthread);
+    return 0;
+}
+
+int pthread_equal(pthread_t t1, pthread_t t2)
+{
+    return t1 == t2;
+}
+
+int pthread_cond_init(pthread_cond_t* __restrict cond, const pthread_condattr_t* __restrict attr)
+{
+    if (!cond)
+    {
+        errno = EINVAL;
+        return -1;
+    }
+    int rc = _es_syscall(0, 0, CREATE_MONITOR, 0);
+    if (rc == -1)
+    {
+        return -1;
+    }
+    *cond = (pthread_cond_t) rc;
+    return 0;
+}
+
+int pthread_cond_destroy(pthread_cond_t* cond)
+{
+    if (!cond)
+    {
+        errno = EINVAL;
+        return -1;
+    }
+    _es_release((void*) *cond);
+    return 0;
+}
+
+int pthread_cond_signal(pthread_cond_t* cond)
+{
+    if (!cond)
+    {
+        errno = EINVAL;
+        return -1;
+    }
+    return _es_syscall((void*) *cond, 0, NOTIFY, 0);
+}
+
+int pthread_cond_broadcast(pthread_cond_t* cond)
+{
+    if (!cond)
+    {
+        errno = EINVAL;
+        return -1;
+    }
+    return _es_syscall((void*) *cond, 0, NOTIFY_ALL, 0);
+}
+
+int pthread_cond_wait(pthread_cond_t* __restrict cond, pthread_mutex_t* __restrict mutex)
+{
+    if (!cond || !mutex)
+    {
+        errno = EINVAL;
+        return -1;
+    }
+    assert(mutex->owner == pthread_self());
+    int count = mutex->count;
+    mutex->count = 1;
+    _es_syscall((void*) *cond, 0, LOCK, 0);
+    pthread_mutex_unlock(mutex);
+    _es_syscall((void*) *cond, 0, WAIT, 0);
+    _es_syscall((void*) *cond, 0, UNLOCK, 0);
+    pthread_mutex_lock(mutex);
+    mutex->count = count;
+    return 0;
+}
+
+int pthread_cond_timedwait(pthread_cond_t* __restrict cond, pthread_mutex_t* __restrict mutex, const struct timespec* __restrict abstime)
+{
+    if (!cond || !mutex)
+    {
+        errno = EINVAL;
+        return -1;
+    }
+    long long timeout = abstime->tv_sec * 10000000LL + abstime->tv_nsec / 100;
+    assert(mutex->owner == pthread_self());
+    int count = mutex->count;
+    mutex->count = 1;
+    _es_syscall((void*) *cond, 0, LOCK, 0);
+    pthread_mutex_unlock(mutex);
+    long long now = _es_syscall(0, 0, GET_NOW, 0);
+    now -= 621355968000000000LL;    // 1970-01-01 00:00:00
+    timeout -= now;
+    int rc = _es_syscall((void*) *cond, 0, WAIT_TIMEOUT, (va_list) &timeout);
+    _es_syscall((void*) *cond, 0, UNLOCK, 0);
+    pthread_mutex_lock(mutex);
+    mutex->count = count;
+    if (rc == 0)
+    {
+        errno = ETIMEDOUT;
+        return -1;
+    }
+    return 0;
+}
