+long long _es_syscall(void* self, void* base, int m, va_list ap)
+{
+    long long result;
+    int error = 0;
+
+    __asm__ __volatile__ (
+        "int    $65"
+        : "=A"(result), "=c" (error) : "a"(self), "d"(m), "c"(ap), "S"(base));
+
+    if (error)
+    {
+        errno = error;
+        return -1;
+    }
+    return result;
