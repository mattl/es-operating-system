+ 
+ #include <fcntl.h>
+ 
+ #include <errno.h>
+ #include <stdarg.h>
+ #include "es-syscall.h"
+ 
+ int __attribute__((weak))
+ _open(const char* path, int flags, ...)
+ {
+     void* context;
+     void* unknown;
+     void* file;
+     void* stream;
+     unsigned int attr;
+     long long size;
+ 
+     if (!path)
+     {
+         errno = EFAULT;
+         return -1;
+     }
+     if (*path == '/')
+     {
+         context = _es_getRoot();
+         ++path;
+     }
+     else
+     {
+         context = _es_getCurrent();
+     }
+     if (!context)
+     {
+         errno = EACCES;
+         return -1;
+     }
+ 
+     if (flags & O_CREAT)
+     {
+         // Create a file
+         unknown = _es_bind(context, path, 0);
+         if (!unknown)
+         {
+             if (flags & O_EXCL)
+             {
+                 _es_release(context);
+                 errno = EEXIST;
+                 return -1;
+             }
+             flags &= ~O_CREAT;  // O_CREAT has no meaning.
+         }
+     }
+     if (!(flags & O_CREAT))
+     {
+         unknown = _es_lookup(context, path);
+     }
+     if (!unknown)
+     {
+         _es_release(context);
+         errno = ENOENT;
+         return -1;
+     }
+     _es_release(context);
+ 
+     context = _es_queryInterface(unknown, &_es_contextIid);
+     if (context)
+     {
+         static const char* arg = "";
+         void* iter;
+ 
+         iter = (void*) _es_syscall(context, 0, LIST, (va_list) &arg);
+         _es_release(context);
+         if (!iter)
+         {
+             errno = EACCES;
+             return -1;
+         }
+         return (int) iter;  // for getdents()
+     }
+ 
+     file = _es_queryInterface(unknown, &_es_fileIid);
+     if (file)
+     {
+         if (flags & O_CREAT)
+         {
+             va_list ap;
+             mode_t mode;
+ 
+             va_start(ap, flags);
+             mode = va_arg(ap, mode_t);
+             va_end(ap);
+             if (mode & S_IWUSR)
+             {
+                 attr = 0;
+             }
+             else
+             {
+                 attr = IFILE_READONLY;
+             }
+             _es_syscall(file, 0, SET_ATTRIBUTES, (va_list) &attr);
+         }
+         else
+         {
+             attr = _es_syscall(file, 0, GET_ATTRIBUTES, 0);
+         }
+         if ((attr & IFILE_READONLY) && (flags & (O_WRONLY | O_RDWR)) ||
+             (attr & IFILE_DIRECTORY))
+         {
+             _es_release(file);
+             errno = EACCES;
+             return -1;
+         }
+ 
+         stream = (void*) _es_syscall(file, 0, GET_STREAM, 0);
+         _es_release(file);
+     }
+     else
+     {
+         stream = _es_queryInterface(unknown, &_es_streamIid);
+     }
+     _es_release(unknown);
+ 
+     if (!stream)
+     {
+         errno = EACCES;
+         return -1;
+     }
+ 
+     if (flags & O_TRUNC)
+     {
+         size = 0;
+         _es_syscall(stream, 0, SET_SIZE, (va_list) &size);
+     }
+ 
+     if (flags & O_APPEND)
+     {
+         size = _es_syscall(stream, 0, GET_SIZE, 0);
+         _es_syscall(stream, 0, SET_POSITION, (va_list) &size);
+     }
+ 
+     return (int) stream;
+ }
