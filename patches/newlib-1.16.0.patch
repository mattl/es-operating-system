+
+#include <sys/time.h>
+#include "es-syscall.h"
+
+int __attribute__((weak))
+_gettimeofday(struct timeval* tv, struct timezone* tz)
+{
+    if (tv)
+    {
+        long long now = _es_syscall(0, 0, GET_NOW, 0);
+
+        now -= 621355968000000000LL;    // 1970-01-01 00:00:00
+        tv->tv_sec = now / 10000000;
+        now %= 10000000;
+        tv->tv_usec = now * 100;
+    }
+
+    if (tz)
+    {
+        tz->tz_minuteswest = 0;
+        tz->tz_dsttime = 0;
+    }
+}
