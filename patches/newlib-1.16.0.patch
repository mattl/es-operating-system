+
+int __attribute__((weak))
+mkdir(const char* path, mode_t mode)
+{
+    void* context;
+    void* subcontext;
+
+    if (!path)
+    {
+        errno = EFAULT;
+        return -1;
+    }
+    if (*path == '/')
+    {
+        context = _es_getRoot();
+        ++path;
+    }
+    else
+    {
+        context = _es_getCurrent();
+    }
+    if (!context)
+    {
+        errno = EACCES;
+        return -1;
