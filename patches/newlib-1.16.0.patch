+ */
+
+#include <errno.h>
+#include <time.h>
+#include "es-syscall.h"
+
+int __attribute__((weak))
+nanosleep(const struct timespec* req, struct timespec* rem)
+{
+    long long time;
+
+    if (!req)
+    {
+        errno = EFAULT;
+        return -1;
+    }
+
+    time = (req->tv_sec * 10000000LL) + (req->tv_nsec / 100);
+
+    void* current = (void*) _es_syscall(0, 0, CURRENT_THREAD, 0);
+    _es_syscall(current, 0, THREAD_SLEEP, (va_list) &time);
+    _es_release(current);
+
+    if (rem)
+    {
+        rem->tv_sec = 0;
+        rem->tv_nsec = 0;
+    }
+
+    return 0;
+}
diff -burN newlib-1.16.0.org/libgloss/i386-es/open.c newlib-1.16.0/libgloss/i386-es/open.c
--- newlib-1.16.0.org/libgloss/i386-es/open.c	1970-01-01 09:00:00.000000000 +0900
