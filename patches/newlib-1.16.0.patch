+ int __attribute__((weak))
+ nanosleep(const struct timespec* req, struct timespec* rem)
+ {
+     long long time;
+ 
+     if (!req)
+     {
+         errno = EFAULT;
+         return -1;
+     }
+ 
+     time = (req->tv_sec * 10000000LL) + (req->tv_nsec / 100);
+ 
