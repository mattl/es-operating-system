+#include <errno.h>
+#include <fcntl.h>
+#include <stdarg.h>
+#include <string.h>
+#include <stdlib.h>
+#include <sys/times.h>
+#include "es-syscall.h"
+
+// Functions that are also defined inside the kernel are declared to be weak.
+#pragma weak _close
+
+int _close(int file)
+{
+    void* object = 0;
+    switch (file)
+    {
+    case 0: // stdin
+        object = _es_streamStdin;
+        _es_streamStdin = 0;
+        break;
+    case 1: // stdout
+        object = _es_streamStdout;
+        _es_streamStdout = 0;
+        break;
+    case 2: // stderr
+        object = _es_streamStderr;
+        _es_streamStderr = 0;
+        break;
+    default:
+        object = (void*) file;
+        break;
+    }
+    if (!object)
+    {
+        errno = EBADF;
+        return -1;
+    }
+    _es_syscall(object, 0, RELEASE, 0);
+    return 0;
+}
