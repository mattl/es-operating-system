+ {
+     return (void*)_es_syscall(0, 0, MAP, (va_list) &start);
+ }
+ 
+ void _es_unmap(const void* start, long long length)
+ {
+     return (void*)_es_syscall(0, 0, UNMAP, (va_list) &start);
+ }
+ 
+ //
+ // IContext
+ //
+ void* _es_lookup(void* context, const char* name)
+ {
+     return (void*) _es_syscall(context, 0, LOOKUP, (va_list) &name);
+ }
+ 
+ void* _es_bind(void* context, const char* name, void* object)
+ {
+     return (void*) _es_syscall(context, 0, BIND, (va_list) &name);
+ }
+ 
+ int _es_rename(void* context, const char* oldName, const char* newName)
+ {
+     return (int) _es_syscall(context, 0, RENAME, (va_list) &oldName);
+ }
+ 
+ //
+ // Misc.
+ //
+ 
+ void _es_panic(const char* file, int line, const char* msg, ...)
+ {
+     struct frame_t
+     {
+         struct frame_t* prev;
+         void*           pc;
+     };
+ 
+     va_list marker;
+ 
+     va_start(marker, msg);
+     vprintf(msg, marker);
+     va_end(marker);
+     printf(" in \"%s\" on line %d.\n", file, line);
+ 
+     struct frame_t* frame = (struct frame_t*) (&file - 2);
+     while (frame)
+     {
+         printf("%p %p\n", frame->pc, frame->prev);
+         frame = frame->prev;
+     }
+ 
+     _exit(EXIT_FAILURE);
+ }
