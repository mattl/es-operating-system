+ #include <stdio.h>
+ 
+ #include <errno.h>
+ #include "es-syscall.h"
+ 
+ int __attribute__((weak))
+ _rename(const char* oldpath, const char* newpath)
+ {
+     void* context;
+     int rc;
+     int i;
+     char path[2048];
+ 
+     if (!oldpath || !newpath)
+     {
+         errno = EFAULT;
+         return -1;
+     }
+ 
+     for (i = 0; oldpath[i] && newpath[i]; ++i)
+     {
+         if (oldpath[i] != newpath[i])
+         {
+             break;
+         }
+     }
+     while (0 < i)
+     {
+         --i;
+         if (oldpath[i] == '/')
+         {
+             ++i;
+             break;
+         }
+     }
+     if (oldpath[i] == '\0')
+     {
+         errno = ENOENT;
+         return -1;
+     }
+ 
+     if (sizeof path <= i)
+     {
+         errno = ENAMETOOLONG;
+         return -1;
+     }
+     if (0 < i)
+     {
+         memmove(path, oldpath, i);
+         path[i - 1] = '\0';
+     }
+     else
+     {
+         path[0] = '\0';
+     }
+ 
+     if (*oldpath == '/')
+     {
+         context = _es_getRoot();
+     }
+     else
+     {
+         context = _es_getCurrent();
+     }
+     if (!context)
+     {
+         errno = EACCES;
+         return -1;
+     }
+ 
+     if (*path)
+     {
+         void* unknown = _es_lookup(context, (*path == '/') ? (path + 1) : path);
+         _es_release(context);
+         if (!unknown)
+         {
+             errno = ENOENT;
+             return -1;
+         }
+ 
+         context = _es_queryInterface(unknown, &_es_contextIid);
+         _es_release(unknown);
+         if (!context)
+         {
+             errno = ENOTDIR;
+             return -1;
+         }
+     }
+ 
+     rc = _es_rename(context, oldpath + i, newpath + i);
+     _es_release(context);
+ 
+     return rc;
+ }
