+ int __attribute__((weak))
+ _stat(const char* path, struct stat* statbuf)
+ {
+     void* context;
+     void* unknown;
+     void* file;
+     void* stream;
+     void* socket;
+ 
+     if (!path)
+     {
+         errno = EFAULT;
+         return -1;
+     }
+     if (*path == '/')
+     {
+         context = _es_getRoot();
+         ++path;
+     }
+     else
+     {
+         context = _es_getCurrent();
+     }
+     if (!context)
+     {
+         errno = EACCES;
+         return -1;
+     }
+ 
+     unknown = _es_lookup(context, path);
+     _es_release(context);
+     if (!unknown)
+     {
+         errno = ENOENT;
+         return -1;
+     }
+ 
+     memset(statbuf, 0, sizeof(struct stat));
+     if ((file = _es_queryInterface(unknown, &_es_fileIid)) != 0)
+     {
+         unsigned int attr = _es_syscall(file, 0, GET_ATTRIBUTES, 0);
+         if (attr & IFILE_READONLY)
+         {
+             statbuf->st_mode |= (S_IRUSR | S_IXUSR);
+         }
+         else
+         {
+             statbuf->st_mode |= S_IRWXU;
+         }
+         if (attr & IFILE_DIRECTORY)
+         {
+             statbuf->st_mode |= S_IFDIR;
+         }
+         else
+         {
+             statbuf->st_mode |= S_IFREG;
+         }
+         statbuf->st_size = _es_syscall(file, 0, GET_FILESIZE, 0);
+         statbuf->st_atime = _es_syscall(file, 0, GET_LASTACCESSTIME, 0);
+         statbuf->st_mtime = _es_syscall(file, 0, GET_LASTWRITETIME, 0);
+         statbuf->st_ctime = _es_syscall(file, 0, GET_CREATIONTIME, 0);
+         _es_release(file);
+     }
+     else if ((socket = _es_queryInterface(unknown, &_es_socketIid)) != 0)
+     {
+         statbuf->st_mode |= S_IFSOCK;
+         _es_release(socket);
+     }
+     else if ((stream = _es_queryInterface(unknown, &_es_streamIid)) != 0)
+     {
+         if (_es_syscall(stream, 0, GET_POSITION, 0) == -1)
+         {
+             statbuf->st_mode |= S_IFCHR;
+         }
+         else
+         {
+             statbuf->st_mode |= S_IFREG;
+         }
+         statbuf->st_size = _es_syscall(stream, 0, GET_SIZE, 0);
+         _es_release(stream);
+     }
+ 
+     _es_release(unknown);
+     return 0;
+ }
