#ifndef NINTENDO_ES_DEVICE_IREMOVABLEMEDIA_IDL_INCLUDED
#define NINTENDO_ES_DEVICE_IREMOVABLEMEDIA_IDL_INCLUDED

#include "es/base/IInterface.idl"

module es
{
    /**
     * This interface represents a removable media.
     */
    interface IRemovableMedia : IInterface
    {
        #pragma ID IRemovableMedia = "DCE:83a9a800-25dc-11db-9c02-0009bf000001";

        /**
         * Ejects this removable media.
         */
        long eject();
        /**
         * Loads this removable media.
         */
        long load();
        /**
         * Locks this removable media.
         */
        long lock();
        /**
         * Unlocks this removable media.
         */
        long unlock();
    };
};

#endif // NINTENDO_ES_DEVICE_IREMOVABLEMEDIA_IDL_INCLUDED

